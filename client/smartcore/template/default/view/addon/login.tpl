<h1 class="title">
    Đăng nhập
</h1>
<div>
    <form id="frmLogin">
        <table class="table-form">
            <tr>
                <td width="10%"><label>Tên đăng nhập</label></td>
                <td><input type="text" class="form-control" id="username" name="username"></td>
            </tr>
            <tr>
                <td><label>Mật khẩu</label></td>
                <td><input type="password" class="form-control" id="password" name="password"></td>
            </tr>

        </table>
        <div class="elife-center">
            <button type="button" class="btn form-control" id="btnSend">Gửi</button>
            <button type="reset" class="btn form-control">Làm lại</button>
        </div>

    </form>
</div>